   <rico:RecordResource rdf:about="recordResource/-aspace_a95521c0ce22ceb019cc325ef093ba29">
      <rico:includedIn rdf:resource="recordResource/-top"/>
      <rdf:type rdf:resource="https://www.ica.org/standards/RiC/ontology#RecordSet"/>
      <rico:hasRecordSetType rdf:resource="https://www.ica.org/standards/RiC/vocabularies/recordSetTypes#Series"/>
      <rico:title xml:lang="en">Series IV. Technical Materials</rico:title>
      <rdfs:label xml:lang="en">Series IV. Technical Materials</rdfs:label>
      <rico:beginningDate rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">1975</rico:beginningDate>
      <rico:endDate rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">1991</rico:endDate>
      <rico:date xml:lang="en">1975-1991</rico:date>
      <rico:scopeAndContent rdf:parseType="Literal">
         <html:p xml:lang="en">Series IV consists of handbooks, safety manuals, safety guidelines, and other materials produced by health and safety organizations including the Environmental Protection Agency, Occupational Safety and Health Administration, City Department of Environmental Variance, National Lighting Bureau, and the New York Committee for Occupation Safety and Health. The contents cover a variety of hazardous substances and related issues that affected members of Local 1-2: radiation exposure, asbestos, PCBs (polychlorinated biphenyls), clean air, lead, pole-mounted transformers, fiberglass wool insulation, cumulative trauma disorders, electrical safety, cadmium, oxygen masks, electromagnetic fields, trench excavation, fire prevention, industrial hygiene, lighting, and others.</html:p>
      </rico:scopeAndContent>
      <rico:hasInstantiation>
         <rico:Instantiation rdf:about="instantiation/-aspace_a95521c0ce22ceb019cc325ef093ba29-i1">
            <rico:instantiates rdf:resource="recordResource/-aspace_a95521c0ce22ceb019cc325ef093ba29"/>
            <rico:title xml:lang="en">Series IV. Technical Materials</rico:title>
            <rdfs:label xml:lang="en">Series IV. Technical Materials</rdfs:label>
            <rico:instantiationExtent rdf:parseType="Literal">
               <html:p xml:lang="en">1.5 Linear Feet</html:p>
            </rico:instantiationExtent>
            <rico:instantiationExtent rdf:parseType="Literal">
               <html:p xml:lang="en">in 3 manuscript boxes.</html:p>
            </rico:instantiationExtent>
